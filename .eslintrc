extends: "airbnb-base"
rules:
  valid-jsdoc: 1
  no-new: 0
  comma-dangle: 0
  arrow-parens: 0
  padded-blocks: 0
  arrow-body-style: 0
  prefer-rest-params: 0
  object-curly-spacing: 0
  no-use-before-define: 0
  class-methods-use-this: 0
  import/prefer-default-export: 0
  import/no-named-as-default-member: 0
